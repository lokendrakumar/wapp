  <% var answers = applicantData.survey_participant.posts %>
  
  <div class="row">    
    <% answers.forEach(function (answer) {%>
      <div class="col-md-3">
        <% var panelId = componentId('playableAnswerSm', answer.id) %>
        <div class="panel fx m-ws-bottom np" id="<%= panelId %>" style="position: relative;width: 244px">
        <script>
          $(function () {
            app.components.surveyVideoAnswers($('#<%= panelId %>'));
          });
        </script>   
        
        <video src="<%= answer.answer.media_urls.original %>" class="img-f nm" style="display:block; width:242px;" data-uuid="video-<%= panelId %>" poster="<%= answer.answer.media.thumbnail_url %>">
        </video> 
        
        <div class='overlay text-center' style="width:242px">
          <div style="position:relative; height:100%">
            <div>
              <h6> <%= answer.question.body %> </h6>
              <span class="glyphicon glyphicon-play overlay-play-icon" aria-hidden="true" style="margin-top:42%"></span>
            </div>
          </div>
        </div>
      </div>
    </div>
    <% }) %>
  </div>

  